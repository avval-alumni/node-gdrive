{
  "name": "node-gdrive",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "email": "d0whc3r@gmail.com",
    "name": "d0whc3r",
    "url": "https://github.com/d0whc3r"
  },
  "bin": {
    "gdrive": "./bin/cli.js"
  },
  "repository": "https://github.com/d0whc3r/node-gdrive.git",
  "keywords": [
    "node",
    "google",
    "drive",
    "gdrive",
    "backup",
    "copy",
    "googleapi"
  ],
  "scripts": {
    "dev": "dotenv -e .env -- node -r ts-node/register -r tsconfig-paths/register tests/index.ts",
    "cli": "dotenv -e .env -- node -r ts-node/register -r tsconfig-paths/register cli/cli.ts",
    "lint": "tslint -p .",
    "build:dist": "rimraf dist && webpack --config webpack/webpack.config.js",
    "build:cli": "rimraf bin && webpack --config webpack/webpack.config-cli.js",
    "build": "yarn build:dist && yarn build:cli"
  },
  "dependencies": {
    "archiver": "3.0.0",
    "googleapis": "38.0.0",
    "mime-types": "2.1.22",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "colors": "1.3.3",
    "command-line-args": "5.0.2",
    "command-line-usage": "5.0.5",
    "dotenv-cli": "1.4.0",
    "glob": "7.1.3",
    "jest": "24.5.0",
    "mysqldump": "2.2.1",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tsconfig-paths": "3.8.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.14.0",
    "typescript": "3.3.3333",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-merge": "4.2.1"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "main": "dist/gdrive.js",
  "types": "types/index.d.ts"
}
